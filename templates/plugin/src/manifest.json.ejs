{
  "manifest_version": 1,
  "version": 1,
  "type": "APP",
  "desktop": {
    "js": [
    <%_ if (library.react) { -%>
      "https://unpkg.com/react@17/umd/react.production.min.js",
      "https://unpkg.com/react-dom@17/umd/react-dom.production.min.js",
    <%_ } else if (library.vue) { -%>
      "https://cdn.jsdelivr.net/npm/vue@<%- library.vue === 2 ? '2.6.14' : '3.2.23' %>",
    <%_ } -%>
      "$desktop"
    ],
    "css": [
      "css/51-modern-default.css",
      "$desktop"
    ]
  },
  "icon": "image/icon.png",
  "config": {
    "html": "html/config.html",
    "js": [
    <%_ if (library.react) { -%>
      "https://unpkg.com/react@17/umd/react.production.min.js",
      "https://unpkg.com/react-dom@17/umd/react-dom.production.min.js",
    <%_ } else if (library.vue) { -%>
      "https://cdn.jsdelivr.net/npm/vue@<%- library.vue === 2 ? '2.6.14' : '3.2.23' %>",
    <%_ } -%>
      "$config"
    ],
    "css": [
      "css/51-modern-default.css",
      "$config"
    ],
    "required_params": [
      "message"
    ]
  },
  "name": {
    "en": "<%- name %>",
    "ja": "<%- name %>",
    "zh": "<%- name %>"
  },
  "description": {
    "en": "<%- description ? description : name %>",
    "ja": "<%- description ? description : name %>",
    "zh": "<%- description ? description : name %>"
  }
}
